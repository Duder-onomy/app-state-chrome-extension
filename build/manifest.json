{
    "manifest_version": 2,
    "name": "app-state-chrome-extension",
    "short_name" : "App-State",
    "description": "app state chrome extension",
    "version": "0.0",
    "devtools_page": "devtools.html",
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "permissions": [
        "tabs",
        "http://*/*",
        "https://*/*"
    ],
    "icons": {
        "128": "icon128.png"
    }
}
